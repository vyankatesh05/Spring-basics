<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   
   <!-- To enable annotations in the program-->
   <context:annotation-config/>
   
   
   <!-- bean definitions go here -->
   <bean class="core.spring.auto.wired.Annotation.Address" name="address11">
	   <property name="street" value="Satara Road"/>
	   <property name="city" value="Pune"/>
	   <property name="country" value="India"/>
   </bean>
  
   <bean class="core.spring.auto.wired.Annotation.Address" name="address123">
	   <property name="street" value="Paud Road"/>
	   <property name="city" value="Nashik"/>
	   <property name="country" value="INDIA"/>
   </bean>
   
<bean class="core.spring.auto.wired.Annotation.Employee" name="emp" />  


</beans>