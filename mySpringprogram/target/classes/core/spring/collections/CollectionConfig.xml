<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="core.spring.collections.EmployeeCollection" name="emp">
		<property name="name" value="Vyankatesh" />

		<property name="phoneno">
			<list>
				<value>9794651</value>
				<value>6543335</value>
			</list>
		</property>

		<property name="address">
			<set>
				<value>Pune</value>
				<value>Mumbai</value>
			</set>
		</property>

	</bean>

	<bean class="core.spring.collections.User" name="user">
		<property name="userID" value="1001" />
		<property name="password" value="sevenmentors" />
	</bean>
</beans>